            <label for="input__checked-checkbox">
                <input
                    checked
                    id="input__checked-checkbox"
                    name="checked-checkbox"
                    type="checkbox" />
                Checked Checkbox
            </label>

            <label for="input__indeterminate-checkbox">
                <input
                    indeterminate
                    id="input__indeterminate-checkbox"
                    name="indeterminate-checkbox"
                    type="checkbox" />
                Indeterminate Checkbox
            </label>

            <fieldset>
                <legend>Pizza Toppings</legend>

                <label for="ham">
                    <input
                        id="ham"
                        name="toppings"
                        type="checkbox" />
                    Ham
                </label>

                <label for="pepperoni">
                    <input
                        id="pepperoni"
                        name="toppings"
                        type="checkbox" />
                    Pepperoni
                </label>

                <label for="mushrooms">
                    <input
                        id="mushrooms"
                        name="toppings"
                        type="checkbox" />
                    Mushrooms
                </label>

                <label for="olives">
                    <input
                        id="olives"
                        name="toppings"
                        type="checkbox" />
                    Olives
                </label>

            </fieldset>

            <label for="input__radio">
                <input
                    id="input__radio"
                    name="radio"
                    type="radio" />
                Radio Button
            </label>

            <label for="input__checked-radio">
                <input
                    checked
                    id="input__checked-radio"
                    name="checked-radio"
                    type="radio" />
                Checked Radio Button
            </label>

            <fieldset>
                <legend>Beverage Size</legend>

                <label for="small">
                    <input
                        id="small"
                        name="beverage"
                        type="radio" />
                    Small
                </label>

                <label for="medium">
                    <input
                        id="medium"
                        name="beverage"
                        type="radio" />
                    Medium
                </label>

                <label for="large">
                    <input
                        id="large"
                        name="beverage"
                        type="radio" />
                    Large
                </label>

                <label for="x-large">
                    <input
                        id="x-large"
                        name="beverage"
                        type="radio" />
                    Large
                </label>

            </fieldset>

        </fieldset><!-- End of #subsection__checkboxes-and-radios -->

        <fieldset id="subsection__buttons" role="region" aria-labelledby="subtitle__buttons"><!-- Start of #subsection__buttons -->
            <legend id="subtitle__buttons" role="heading" aria-level="3">Buttons</legend>

            <button>
                Button
            </button>

            <button
                data-button-priority="primary">
                Primary Button
            </button>

            <button
                data-button-priority="secondary">
                Secondary Button
            </button>

            <input
                name=button""
                value="Input Button"
                type="button" />

            <button aria-label="Icon Button">
                &#9742;
            </button>

            <button>
                Button with <abbr>SVG</abbr> icon
                <svg
                    aria-hidden="true"
                    height="16"
                    width="16"
                    viewBox="0 0 16 16"
                    xmlns="http://www.w3.org/2000/svg">
                    <path fill="#000000" fill-rule="evenodd" d="M8,16 C12.418278,16 16,12.418278 16,8 C16,3.581722 12.418278,0 8,0 C3.581722,0 0,3.581722 0,8 C0,12.418278 3.581722,16 8,16 Z M8.28116557,3 C8.58745219,3 8.8466139,3.10719871 9.05865848,3.32159934 C9.27070306,3.53599997 9.37672376,3.79398367 9.37672376,4.09555819 C9.37672376,4.3971327 9.26952505,4.6539384 9.05512442,4.86598298 C8.84072379,5.07802756 8.58274009,5.18404826 8.28116557,5.18404826 C7.97959106,5.18404826 7.72278536,5.07802756 7.51074078,4.86598298 C7.2986962,4.6539384 7.1926755,4.3971327 7.1926755,4.09555819 C7.1926755,3.79398367 7.2986962,3.53599997 7.51074078,3.32159934 C7.72278536,3.10719871 7.97959106,3 8.28116557,3 Z M9.270702,6.20185716 L9.270702,11.4393321 C9.270702,11.9058302 9.32489036,12.2038662 9.4332687,12.333449 C9.54164704,12.4630318 9.75368844,12.5372462 10.0693993,12.5560947 L10.0693993,12.8105469 L6.5,12.8105469 L6.5,12.5560947 C6.79215031,12.5466704 7.00890374,12.4618539 7.1502668,12.3016424 C7.24450883,12.1932641 7.29162914,11.9058302 7.29162914,11.4393321 L7.29162914,7.58014004 C7.29162914,7.11364196 7.23744078,6.81560599 7.12906244,6.6860232 C7.0206841,6.5564404 6.81099872,6.48222591 6.5,6.4633775 L6.5,6.20185716 L9.270702,6.20185716 Z"/>
                </svg>
            </button>

            <span
                role="button"
                tabindex="0">
                Text Button
            </span>

            <h4>Notes:</h4>
            <ul>
                <li>
                    <small>Add <code>pointer-events: none;</code> to <a rel="external noopener" href="https://twitter.com/stowball/status/857707052545613824">inline SVG icons placed in buttons</a>.</small>
                </li>
                <li>
                    <small>Use <code>tabindex="0"</code> in conjunction with <code>role="button"</code> to ensure that the text button is focusable. Use <code>cursor: pointer;</code> in the CSS to communicate that it is interactable. Learn more about <a rel="external noopener" href="https://css-tricks.com/use-button-element/"><cite>When To Use The Button Element</cite></a>.</small>
                </li>
            </ul>

        </fieldset><!-- End of #subsection__buttons -->

        <fieldset id="subsection__output" role="region" aria-labelledby="subtitle__output"><!-- Start of #subsection__output -->
            <legend id="subtitle__output" role="heading" aria-level="3">Output</legend>

            <output
                for="output"
                name="output">
                Output
            </output>

            <progress
                max="100"
                value="65">
            </progress>

            <meter
                min="200"
                max="500"
                value="350">
                350 degrees
            </meter>

        </fieldset><!-- End of #subsection__output -->

        <fieldset id="subsection__lockup" role="region" aria-labelledby="subtitle__lockup"><!-- Start of #subsection__lockup -->
            <legend id="subtitle__lockup" role="heading" aria-level="3">Lockup</legend>

            <label for="input__lockup">
                Your Name
                <span>This is what we'll use to refer to you if we have to contact you</span>
                <input
                    id="input__lockup"
                    name="lockup"
                    placeholder="Firstname Lastname "
                    type="text" />
            </label>
            <span>Can we send you periodic messages about your account?</span>
            <label for="input__inline-interaction">
                <input
                    id="input__inline-interaction"
                    name="inline-interaction"
                    type="checkbox" />
                I agree
            </label>

        </fieldset><!-- End of #subsection__lockup -->

        <fieldset id="subsection__states-and-validation" role="region" aria-labelledby="subtitle__states-and-validation"><!-- Start of #subsection__states-and-validation -->
            <legend id="subtitle__states-and-validation" role="heading" aria-level="3">States and Validation</legend>
            <div id="subsection__input" role="region" aria-labelledby="subtitle__input"><!-- Start of #subsection__input -->
                <h4 id="subtitle__input">Input</h4>
                <label for="input__required">
                    Required
                    <input
                        required
                        id="input__required"
                        name="required"
                        type="text" />
                </label>

                <label for="input__readonly">
                    Readonly
                    <input
                        readonly
                        value="Read Only"
                        id="input__readonly"
                        name="readonly"
                        type="text" />
                </label>

                <label for="input__loading">
                    Loading
                    <input
                        id="input__loading"
                        name="loading"
                        aria-busy="true"
                        data-input-state="loading"
                        type="text" />
                </label>

                <label for="input__disabled">
                    Disabled
                    <input
                        disabled
                        id="input__disabled"
                        name="disabled"
                        type="text" />
                </label>

                <label for="input__valid">
                    Valid
                    <input
                        id="input__valid"
                        name="valid"
                        aria-invalid="false"
                        type="text" />
                </label>

                <label for="input__invalid">
                    Invalid
                    <input
                        id="input__invalid"
                        name="invalid"
                        aria-invalid="true"
                        type="text" />
                </label>

                <label for="input__warning">
                    Warning
                    <input
                        id="input__warning"
                        name="warning"
                        data-input-state="warning"
                        type="text" />
                </label>

                <label for="input__search-with-autosave">
                    Search with Autosave
                    <input
                        autosave
                        id="input__search-with-autosave"
                        name="search-with-autosave"
                        type="search" />
                </label>

                <label for="input__pattern">
                    Input with Pattern Validation
                    <input
                        pattern="[0-9]{13,16}"
                        id="input__pattern"
                        name="pattern"
                        type="number" />
                </label>

                <label for="input__minlength">
                    Minlength
                    <input
                        id="input__minlength"
                        name="minlength"
                        minlength="5"
                        type="number" />
                </label>

                <label for="input__maxlength">
                    Maxlength
                    <input
                        id="input__maxlength"
                        name="maxlength"
                        maxlength="10"
                        type="number" />
                </label>
            </div><!-- End of #subsection__input -->

            <div id="subsection__buttons" role="region" aria-labelledby="subtitle__buttons"><!-- Start of #subsection__buttons -->
                <h4 id="subtitle__buttons">Buttons</h4>
                <button
                    disabled>
                    Disabled
                </button>

                <button
                    aria-busy="true">
                    Loading
                </button>

                <h5>Notes:</h5>
                <p>
                    <small>Use <code>:hover</code>, <code>:focus</code>, and <code>:active</code> selectors to control the other button states. Learn more about <a rel="external noopener" href="https://medium.com/eightshapes-llc/buttons-in-design-systems-eac3acf7e23#.b1p96hsrw">Buttons in Design Systems</a>.</small>
                </p>
            </div><!-- End of #subsection__buttons -->
        </fieldset><!-- End of #subsection__states-and-validation -->

        <fieldset role="region" aria-labelledby="subtitle__autocomplete" id="subsection__autocomplete"><!-- Start of #subsection__autocomplete -->
