<section id="section__forms" aria-labelledby="title__forms"><!-- Start of #section__forms -->
    <h2 id="title__forms">Forms</h2>

    <form>
        <fieldset id="subsection__text-input" role="region" aria-labelledby="subtitle__text-input"><!-- Start of #subsection__text-input -->
            <legend id="subtitle__text-input" role="heading" aria-level="3">Text Input</legend>

            <label for="input__text">
                Text
                <input
                    id="input__text"
                    name="text"
                    type="text" />
            </label>

            <label for="input__text-with-placeholder">
                Text with Placeholder
                <input
                    id="input__text-with-placeholder"
                    name="placeholder"
                    placeholder="Placeholder"
                    type="text" />
            </label>

            <label for="input__email">
                Email
                <input
                    id="input__email"
                    name="email"
                    placeholder="name@example.com"
                    autocapitalize="off"
                    autocorrect="off"
                    spellcheck="false"
                    type="email" />
            </label>

            <label for="input__password">
                Password
                <input
                    id="input__password"
                    name="password"
                    autocapitalize="off"
                    autocorrect="off"
                    spellcheck="false"
                    type="password" />
            </label>

            <label for="input__url">
                URL
                <input
                    id="input__url"
                    name="url"
                    autocapitalize="off"
                    spellcheck="false"
                    type="url" />
            </label>

            <label for="input__telephone">
                Telephone
                <input
                    id="input__telephone"
                    name="telephone"
                    autocorrect="off"
                    autocomplete="tel"
                    spellcheck="false"
                    type="tel" />
            </label>

            <label for="input__number">
                Number
                <input
                    id="input__number"
                    name="number"
                    type="number" />
            </label>

            <label for="input__search">
                Search
                <input
                    id="input__search"
                    name="search"
                    spellcheck="false"
                    type="search" />
            </label>

            <label for="input__date">
                Date
                <input
                    id="input__date"
                    name="date"
                    type="date" />
            </label>

            <label for="input__datetime-local">
                Datetime-local
                <input
                    id="input__datetime-local"
                    name="datetime-local"
                    type="datetime-local" />
            </label>

            <label for="input__month">
                Month
                <input
                    id="input__month"
                    name="month"
                    type="month" />
            </label>

            <label for="input__time">
                Time
                <input
                    id="input__time"
                    name="time"
                    type="time" />
            </label>

            <label for="input__week">
                Week
                <input
                    id="input__week"
                    name="week"
                    type="week" />
            </label>

            <label for="input__datalist">
                Datalist
                <input
                    list="dog-breeds"
                    id="input__datalist"
                    name="datalist"
                    type="text">
                    <datalist id="dog-breeds">
                        <option value="Beagles"></option>
                        <option value="Boxers"></option>
                        <option value="Bulldogs"></option>
                        <option value="Dachshunds"></option>
                        <option value="French Bulldogs"></option>
                        <option value="German Shepherds"></option>
                        <option value="German Shorthaired Pointers"></option>
                        <option value="Golden Retrievers"></option>
                        <option value="Labrador Retrievers"></option>
                        <option value="Poodles"></option>
                        <option value="Rottweilers"></option>
                        <option value="Siberian Huskies"></option>
                        <option value="Yorkshire Terriers"></option>
                    </datalist>
                </input>
            </label>

            <label for="input__textarea">
                Textarea
                <textarea
                    id="input__textarea"
                    name="textarea"
                    dir="auto">
                </textarea>
            </label>

        </fieldset><!-- End of #subsection__text-input -->

        <fieldset id="subsection__non-text-input" role="region" aria-labelledby="subtitle__non-text-input"><!-- Start of #subsection__non-text-input -->
            <legend id="subtitle__non-text-input" role="heading" aria-level="3">Non-text Input</legend>

            <label for="input__color">
                Color
                <input
                    id="input__color"
                    name="color"
                    type="color" />
            </label>

            <label for="input__file">
                File Input
                <input
                    id="input__file"
                    name="file"
                    type="file" />
            </label>

            <label for="input__range">
                Range Input
                <input
                    id="input__range"
                    name="range"
                    type="range" />
            </label>

        </fieldset><!-- End of #subsection__non-text-input -->

        <fieldset id="subsection__select" role="region" aria-labelledby="subtitle__select"><!-- Start of #subsection__select -->
            <legend id="subtitle__select" role="heading" aria-level="3">Select</legend>

            <label for="input__select">
                Select
